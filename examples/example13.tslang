    eric valeurs -> int[4]
    valeurs[0] = 4
    valeurs[1] = 7
    valeurs[2] = 2
    valeurs[3] = 9

    eric res = somme(valeurs, 4)
    peric("Somme = {res}")
    deschodt 0

